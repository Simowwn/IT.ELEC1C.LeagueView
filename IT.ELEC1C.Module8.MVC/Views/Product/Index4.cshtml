@model List<Users>
@{
    ViewData["Title"] = "Users";
}

<div>
    <table>
        <tr>
            <th width="200px">ID</th>
            <th width="200px">Email</th>
            <th width="200px">Username</th>
            <th width="200px">Password</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Email</td>
                <td>@item.Username</td>
                <td>@item.PasswordHash</td>

                <td></td>
                
            </tr>
        }
    </table>
</div>

